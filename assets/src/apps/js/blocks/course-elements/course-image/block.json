{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "learnpress/course-image",
	"title": "Course Image",
	"category": "learnpress-course-elements",
	"icon": "format-image",
	"description": "Renders template Image Course PHP templates.",
	"textdomain": "learnpress",
	"keywords": [ "image single course", "learnpress" ],
	"ancestor": [ "learnpress/single-course", "learnpress/course-item-template" ],
	"usesContext": [ "lpCourseData" ],
	"attributes": {
		"isLink": {
			"type": "boolean",
			"default": true
		},
		"target": {
			"type": "boolean",
			"default": false
		},
		"size": {
			"type": "string",
			"default": "custom"
		},
		"customWidth": {
			"type": "number",
			"default": 500
		},
		"customHeight": {
			"type": "number",
			"default": 300
		}
	},
	"supports": {
		"multiple": true,
		"html": false,
		"shadow": true,
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"width": false,
				"color": false,
				"radius": false
			}
		}
	}
}
